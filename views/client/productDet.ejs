            const mainIm=document.querySelector('.mainIM')
            //image galarry
            window.onload=()=>{galIm0.classList.add('active')}
            function show(e,i){
                const gal=document.getElementById(`ii${i}`);
                const removeActive=document.querySelector('.active');
                removeActive.classList.remove('active')
                gal.classList.add('active');
                console.log(mainIm);
                mainIm.src=e.src
                

            }

            // image hovering zoom
            const elem = document.querySelector('.imf');
            let x, y, height, width;
            elem.onmouseenter = () => {
                const size = elem.getBoundingClientRect();
                x = size.x;
                y = size.y;
                width = size.width;
                height = size.height;

            }
            elem.onmousemove = e => {
                const horizontel = (e.clientX - x) / width * 100;
                const vertical = (e.clientY - y) / height * 100;
        

                elem.style.setProperty('--x', horizontel + '%')
                elem.style.setProperty('--y', vertical + '%')
              
            }


        </script>
        <style>
            body::-webkit-scrollbar {
                width: 0;
