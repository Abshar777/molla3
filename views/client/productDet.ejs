            .imf {
                overflow: hidden;
            }

            .imf img {

                transform: scale(var(--zoom, 1));
                object-fit: cover;
                transform-origin: var(--x) var(--y);
                transition: cubic-bezier(0.18, 0.9, 0.33, 1.01) 1s;
                /* opacity: 0.3; */
            }

            .imf:hover {
                --zoom: 1.8;
                cursor: crosshair;



            }
